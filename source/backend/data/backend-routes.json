{
	"GET": {
		"TaskFlow/get-worker-info": ["WorkerController", "getWorkerInfoByKey"],
		"TaskFlow/get-worker-info/{workerId:[\\w+_-]+}": [
			"WorkerController",
			"getWorkerInfoById"
		],
		"TaskFlow/get-worker-info?(?:[\\w+\\s_=\\/\\?,%&@-]+)+": [
			"WorkerController",
			"getWorkerInfoByKey"
		]
	},

	"POST": {
		"TaskFlow/create-project": ["ProjectController", "createProject"],

		"TaskFlow/add-worker": ["WorkerController", "addWorker"],
		"TaskFlow/add-phase/{projectId:[\\w+_-]+}": ["PhaseController", "addPhase"],
        "TaskFlow/add-task/{projectId:[\\w+_-]+}": ["TaskController", "addTask"],

		"TaskFlow/edit-project/{projectId:[\\w+_-]+}": ["ProjectController", "editProject"],

		"TaskFlow/cancel-project": ["ProjectController", "cancelProject"],
		"TaskFlow/cancel-phase": ["PhaseController", "cancelPhase"],

		"TaskFlow/terminate-worker": ["WorkerController", "terminateWorker"]
	}
}
