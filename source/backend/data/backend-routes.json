{
	"GET": {
		"TaskFlow/projects/{projectId:[\\w+_-]+}/workers": ["WorkerController", "getWorkerInfoByKey"],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/workers/{workerId:[\\w+_-]+}": [
			"WorkerController",
			"getWorkerInfoById"
		],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/workers?(?:[\\w+\\s_=\\/\\?,%&@-]+)+": [
			"WorkerController",
			"getWorkerInfoByKey"
		]
	},

	"POST": {
		"TaskFlow/projects": [
			"ProjectController", 
			"createProject"
		],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/workers": [
			"WorkerController",
			"addWorker"
		],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/phases": [
			"PhaseController",
			"addPhase"
		],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/tasks": [
			"TaskController",
			"addTask"
		]
	},

	"PUT": {
		"TaskFlow/projects/{projectId:[\\w+_-]+}": [
			"ProjectController",
			"editProject"
		],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/tasks/{taskId:[\\w+_-]+}": [
			"TaskController",
			"editTask"
		],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/workers/{workerId:[\\w+_-]+}": [
			"WorkerController",
			"editWorker"
		]
	},
	
	"PATCH": {
		"TaskFlow/projects/{projectId:[\\w+_-]+}": [
			"ProjectController",
			"editProject"
		],
		"TaskFlow/users/{userId:[\\w+_-]+}": ["UserController", "editUser"],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/tasks/{taskId:[\\w+_-]+}": [
			"TaskController",
			"editTask"
		],
		"TaskFlow/projects/{projectId:[\\w+_-]+}/workers/{workerId:[\\w+_-]+}": [
			"WorkerController",
			"editWorker"
		]
	}
}
