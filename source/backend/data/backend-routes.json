{
    "GET": {
        "TaskFlow/get-worker-info": ["WorkerController", "getWorkerInfoByKey"],
        "TaskFlow/get-worker-info/{workerId:[\\w+_-]+}": ["WorkerController", "getWorkerInfoById"],
        "TaskFlow/get-worker-info?(?:[\\w+\\s_=\\/\\?,%&@-]+)+": ["WorkerController", "getWorkerInfoByKey"]
    },

    "POST": {
        "TaskFlow/cancel-project": ["ProjectController", "cancelProject"],
        "TaskFlow/add-worker": ["WorkerController", "addWorker"],
        "TaskFlow/terminate-worker": ["WorkerController", "terminateWorker"],
        "TaskFlow/add-phase": ["PhaseController", "addPhase"],
        "TaskFlow/cancel-phase": ["PhaseController", "cancelPhase"]
    }
}