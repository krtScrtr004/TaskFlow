{
	"GET": {
		"TaskFlow/": "IndexController",
		"TaskFlow/login": "IndexController",
		"TaskFlow/register": "IndexController",
		"TaskFlow/confirm-email(?:\\?[\\w+\\s_=\\/\\?,%&@-]+)?": ["IndexController", "confirmEmail"],

		"TaskFlow/reset-password": "SingleFormController",
		"TaskFlow/change-password(?:\\?[\\w+\\s_=\\/\\?,%&@-]+)?": "SingleFormController",
		"TaskFlow/create-project": "SingleFormController",
		"TaskFlow/edit-project/{projectId:[\\w+_-]+}": "SingleFormController",
		"TaskFlow/add-task/{projectId:[\\w+_-]+}": "SingleFormController",

		"TaskFlow/home": ["ProjectController", "viewHome"],
		"TaskFlow/project/{projectId:[\\w+_-]+}": [
			"ProjectController",
			"viewOther"
		],
		"TaskFlow/project/{projectId:[\\w+_-]+}/report": [
			"ProjectController", 
			"viewReport"
		],

		"TaskFlow/project/{projectId:[\\w+_-]+}/task(?:\\?[\\w+\\s_=\\/\\?,%&@-]+)?": [
			"TaskController", 
			"viewGrid"
		],		
		"TaskFlow/project/{projectId:[\\w+_-]+}/phase/{phaseId:[\\w+_-]+}/task/{taskId:[\\w+_-]+}": [
			"TaskController",
			"viewInfo"
		],

		"TaskFlow/projects(?:\\?[\\w+\\s_=\\/\\?,%&@-]+)?": [
			"ProjectController",
			"viewGrid"
		],

		"TaskFlow/users(?:\\?[\\w+\\s_=\\/\\?,%&@-]+)?": "UserController",

		"TaskFlow/profile": "ProfileController", 

		"TaskFlow/not-found": ["ErrorController", "notFound"],
		"TaskFlow/forbidden": ["ErrorController", "forbidden"]
	}
}
