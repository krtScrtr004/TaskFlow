{
	"GET": {
		"TaskFlow/": "IndexController",
		"TaskFlow/login": "IndexController",
		"TaskFlow/register": "IndexController",

		"TaskFlow/forget-password": "SingleFormController",
		"TaskFlow/change-password": "SingleFormController",
		"TaskFlow/create-project": "SingleFormController",
		"TaskFlow/edit-project/{projectId:[\\w+_-]+}": "SingleFormController",
		"TaskFlow/add-task/{projectId:[\\w+_-]+}": "SingleFormController",

		"TaskFlow/home": ["ProjectController", "viewProject"],
		"TaskFlow/home/{projectId:[\\w+_-]+}": [
			"ProjectController",
			"viewProject"
		],

		"TaskFlow/project/{projectId:[\\w+_-]+}/task(?:\\?[\\w+\\s_=\\/\\?,%&@-]+)?": "TaskController",
		"TaskFlow/project/{projectId:[\\w+_-]+}/task/{taskId:[\\w+_-]+}": [
			"TaskController",
			"viewTask"
		],

		"TaskFlow/projects(?:\\?[\\w+\\s_=\\/\\?,%&@-]+)?": [
			"ProjectController",
			"viewProjectGrid"
		]
	}
}
